<form action="/quiz">
    <label for="questionCount">How many Questions?(4-8)</label>
    <input type="number" id="questionCount" name="questionCount" min="4" max="8" />
    <br>
        <input type="checkbox" id="Linux" name="Linux">
	    <label for="Linux">Linux</label>
	    <input type="checkbox" id="DevOps" name="DevOps">
	    <label for="DevOps">DevOps</label>
	    <input type="checkbox" id="SQL" name="SQL">
	    <label for="SQL">SQL</label>
	    <input type="checkbox" id="Bash" name="Bash">
	    <label for="Bash">Bash</label>
	<br>
    
    <input type="submit" class="btn btn-default btn-lg" value="Start Quiz">
</form>